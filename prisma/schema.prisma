// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Brands {
  id   String @id @default(cuid())
  name String
  Cars Cars[]
}

model Cars {
  id      String @id @default(cuid())
  model   String
  year    String
  price   Float
  brandId String

  brand Brands @relation(fields: [brandId], references: [id])
}
